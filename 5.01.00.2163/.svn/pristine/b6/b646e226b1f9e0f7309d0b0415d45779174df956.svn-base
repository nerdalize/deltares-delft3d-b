%   Stichting Deltares. All rights reserved.                                     
%                                                                               
%-------------------------------------------------------------------------------
%   http://www.deltaressystems.com
%   $HeadURL$
%   $Id$

for i = 1:length(DimList)
    fprintf('    %-11s: ',DimList{i});
    d = strmatch(DimList{i},{ResDims.Name},'exact');
    if isempty(d)
        fprintf('<missing information>\n');
    else
        Dim = ResDims(d);
        if isfield(Selection,Dim.Name)
            fprintf('%s\n',dimprint(Dim,Selection.(Dim.Name)));
        else
            fprintf('%s\n',dimprint(Dim));
        end
    end
end
